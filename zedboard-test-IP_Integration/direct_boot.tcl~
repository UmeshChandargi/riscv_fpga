## Internal Instruction memory --
## implement processor-internal instruction memory

## boot configuration: false = boot from int/ext (I)MEM

## Change generic parameters of top design
set_property generic {MEM_INT_IMEM_EN =true INT_BOOTLOADER_EN=false} [current_fileset]

## Reset the already prepared Systhesis
reset_run synth_1

# Run synthesis, implementation and bitstream generation
launch_runs impl_1 -to_step write_bitstream -jobs 4
wait_on_run impl_1

